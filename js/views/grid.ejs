<svg xmlns='http://www.w3.org/2000/svg' width='<%= totalW %>' height='<%= totalH %>'>
	<foreignObject width='100%' height='100%'>
		<div id="container" xmlns='http://www.w3.org/1999/xhtml' style="width:100%;height:100%;">
			<% for (var j = 0; j < gridH; j++) { %>
				<% for (var i = 0; i < gridW; i++) { %>	
					<div grid-x="<%= i %>" grid-y="<%= j %>" is-open="0" is-revealed="0" has-flag="0" has-mine="0" style="float:left;background-color:#bdbdbd;border:4px solid #ffffff;border-right:4px solid #696969;border-bottom:4px solid #696969;width:20px;height:20px;">
						<p xmlns="http://www.w3.org/1999/xhtml" style="font-family:courier;font-size:14px;margin:0;padding:2px 0 0 6px;display:none;"></p>
					</div>
				<% } %>
			<% } %>
		</div>
	</foreignObject>
</svg>